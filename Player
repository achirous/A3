
public class Player {
	String playerName;
	Boolean hasWon;
	
	public Player (String name){
		playerName = name;
		
	}
	
	public void setName(String name){
		playerName = name;
	}
	
	public String getName(){
		return playerName;
	}
	
	public void setHasWon(Boolean hw){
		hasWon = hw;
	}
	
	public Boolean getHasWon(){
		return hasWon;
	}
	
	/*public void playerTurn(int x, int y){
		
	}*/

}
